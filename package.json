{
  "name": "ToknXR-CLI",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "toknxr-cli"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "turbo build",
    "build:web": "next build --turbopack",
    "build:cli": "turbo build --filter=toknxr-cli",
    "start": "next start",
    "lint": "turbo lint",
    "lint:fix": "eslint --fix",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "cli:dev": "turbo dev --filter=@goldensheepai/toknxr-cli",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --config .prettierrc.json --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --config .prettierrc.json --write"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.1",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "turbo": "^2.5.8",
    "typescript": "^5"
  }
}
